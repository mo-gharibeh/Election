@model IEnumerable<fstCopy_Proj5.Models.DATE>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.startDateNominate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndDateNominate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.startDateOfElection)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndDateOfElection)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.startDateNominate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndDateNominate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.startDateOfElection)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndDateOfElection)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.ID }) |
                @Html.ActionLink("Details", "Details", new { id = item.ID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.ID })
            </td>
        </tr>
    }

</table>
